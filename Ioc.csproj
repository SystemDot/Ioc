<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
	<TargetFrameworks>netcoreapp3.1; netstandard2.0</TargetFrameworks>
    <AssemblyName>SystemDot.Ioc</AssemblyName>
    <RootNamespace>SystemDot.Ioc</RootNamespace>
	<PackageId>SystemDotIoc</PackageId>
	<Version>2.1.0</Version>
	<Authors>Chris Sampson. James Goodenough</Authors>
	<Company>SystemDot</Company>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<Description>Inversion of control container for Windows</Description>
	<PackageLicenseFile>LICENSE</PackageLicenseFile>
	<RepositoryType>Github</RepositoryType>
	<PackageTags>Ioc</PackageTags>
	<RepositoryUrl>https://github.com/SystemDot/Ioc</RepositoryUrl>
  </PropertyGroup>
	<ItemGroup>
	  <None Include="LICENSE">
	    <Pack>True</Pack>
	    <PackagePath></PackagePath>
	  </None>
	</ItemGroup>
</Project>
